{
  "name": "@craftercms/studio-ui",
  "description": "Services, components, models & utils to build Crafter CMS authoring extensions.",
  "version": "1.0.0-beta.3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/craftercms/studio-ui.git",
    "directory": "ui/app/src"
  },
  "keywords": [
    "content management",
    "craftercms",
    "crafter",
    "studio",
    "cms"
  ],
  "license": "LGPL-3.0-only",
  "proxy": "http://localhost:8080/",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "spaced-comment": [
        "error",
        "always",
        {
          "line": {
            "markers": [
              "/",
              "#"
            ]
          },
          "block": {
            "markers": [
              "!",
              "*",
              "#",
              "function",
              "const",
              "if",
              "export",
              "interface",
              "#__PURE__",
              "@__PURE__"
            ],
            "exceptions": [
              "!",
              "*",
              "#"
            ],
            "balanced": false
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bic": {
    "skip": [
      "build",
      "build_tsc",
      "scripts",
      "README.md",
      "CHANGE_LOG.md",
      ".babelrc"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "run-s --max_old_space_size=4096 build:react build:worker deploy:app",
    "build:install": "yarn",
    "build:react": "react-scripts --max_old_space_size=4096 build",
    "deploy:app": "node scripts/deploy.js",
    "build:worker": "rollup -c rollup.config.js --environment PRODUCTION",
    "build:worker:dev": "rollup -c rollup.config.js --environment PRODUCTION:false",
    "watch:worker": "rollup -w -c rollup.config.js --environment PRODUCTION:false",
    "build:app": "run-s build:react deploy:app",
    "compile": "tsc -p tsconfig.json",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:node": "tsc -p tsconfig.node.json",
    "format:npm": "prettier --config ../../prettier.config.js ./build_tsc/**/*.{js,ts} --write",
    "package:npm": "node scripts/package.js",
    "build:npm": "run-s clean:npm build:tsc build:node clean:models package:npm",
    "clean:npm": "rimraf ./build_tsc ./tsconfig.build.tsbuildinfo",
    "clean:models": "rimraf ./build_tsc/models/**/*.{js,map}",
    "test": "react-scripts test",
    "i18n:extract": "NODE_ENV=production babel ./src --extensions '.ts,.tsx' --out-file /dev/null",
    "i18n:manage": "node scripts/i18n.js",
    "prettier": "node ../../scripts/prettier.js"
  },
  "dependencies": {
    "@date-io/core": "^1.3.6",
    "@date-io/date-fns": "^1.3.11",
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.3.0",
    "@mui/lab": "^5.0.0-alpha.65",
    "@mui/material": "^5.3.0",
    "@mui/styles": "^5.3.0",
    "@mui/x-data-grid": "^5.2.2",
    "@prettier/plugin-xml": "^1.2.0",
    "@reduxjs/toolkit": "^1.7.0",
    "@types/ace": "^0.0.47",
    "@types/graphql": "^14.5.0",
    "@types/marked": "^4.0.1",
    "@types/react-swipeable-views": "^0.13.1",
    "@types/video.js": "^7.3.27",
    "@videojs/vhs-utils": "^2.3.0",
    "autosuggest-highlight": "^3.2.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.25.0",
    "fast-xml-parser": "3.21.1",
    "graphiql": "^1.5.16",
    "graphiql-explorer": "^0.6.3",
    "graphql": "^15.8.0",
    "highlight.js": "^11.3.1",
    "jabber": "^1.2.2",
    "jquery": "^3.6.0",
    "js-cookie": "^3.0.1",
    "marked": "^4.0.8",
    "moment-timezone": "^0.5.34",
    "nanoid": "^3.1.30",
    "notistack": "2.0.3",
    "prettier": "^2.5.1",
    "query-string": "^6.14.1",
    "react": "^17.0.2",
    "react-animations": "^1.0.0",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^3.4.4",
    "react-intl": "^3.9.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "redux": "^4.1.2",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.4.0",
    "typescript": "^4.1.2",
    "uppy": "^2.3.3",
    "video.js": "^7.16.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "7.12.3",
    "@babel/runtime": "^7.15.4",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jquery": "^3.5.11",
    "@types/js-cookie": "^3",
    "@types/prettier": "^2.4.2",
    "@types/react-redux": "^7.1.20",
    "babel-plugin-react-intl": "^5.1.16",
    "build-if-changed": "^1.5.5",
    "eslint": "^7.11.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react-intl-translations-manager": "^5.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0"
  }
}
