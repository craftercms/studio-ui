    "watch": "babel src -d lib --watch",
    "bom": "yarn cyclonedx --output-file bom.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/plugin-transform-object-assign": "^7.24.1",
    "@types/eslint": "^8.56.10",
    "babel-plugin-inline-package-json": "^2.0.0",
    "build-if-changed": "^1.5.5",
    "eslint": "^8.3.0"
  },
  "peerDependencies": {
    "uppy": "^2.13.8"
  },
  "files": [
    "lib/*",
    "types/*"
  ],
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
