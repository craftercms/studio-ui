{
  "name": "@craftercms/studio-guest",
  "version": "1.0.0",
  "private": true,
  "main": "build_tsc/index.js",
  "module": "build_tsc/index.js",
  "typings": "build_tsc/index.d.ts",
  "craftercms": {
    "id": "craftercms.guest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "spaced-comment": [
        "error",
        "always",
        {
          "line": {
            "markers": [
              "/",
              "#"
            ]
          },
          "block": {
            "markers": [
              "!",
              "*",
              "#",
              "function",
              "const",
              "if",
              "export",
              "interface",
              "#__PURE__",
              "@__PURE__"
            ],
            "exceptions": [
              "!",
              "*",
              "#"
            ],
            "balanced": false
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bic": {
    "skip": [
      "build",
      "build_tsc",
      "README.md",
      "CHANGE_LOG.md"
    ]
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "watch": "rollup -c -w",
    "compile": "tsc --build tsconfig.json"
  },
  "dependencies": {
    "@craftercms/classes": "^2.0.0",
    "@craftercms/content": "^2.0.0",
    "@craftercms/search": "^2.0.0",
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@mui/material": "*",
    "lorem-ipsum": "^2.0.4",
    "react": "*",
    "react-dom": "*",
    "typescript": "*",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "*",
    "@babel/core": "*",
    "@babel/preset-typescript": "^7.7.4",
    "@types/tinymce": "^4.6.4",
    "build-if-changed": "*",
    "npm-run-all": "*",
    "rollup": "*",
    "yargs": "^15.3.1"
  }
}
