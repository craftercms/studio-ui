{
  "name": "@craftercms/studio-guest",
  "version": "1.0.0",
  "private": true,
  "main": "build_tsc/index.js",
  "module": "build_tsc/index.js",
  "typings": "build_tsc/index.d.ts",
  "craftercms": {
    "id": "craftercms.guest"
  },
  "dependencies": {
    "@craftercms/classes": "1.2.4",
    "@craftercms/content": "1.2.4",
    "@craftercms/search": "1.2.4",
    "jss": "^10.0.3",
    "jss-preset-default": "^10.0.0",
    "lorem-ipsum": "^2.0.3",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "build": "run-s build:install build:app",
    "build:install": "yarn",
    "build:app": "rollup -c",
    "start": "rollup -c -w",
    "watch": "rollup -c -w",
    "rollup": "rollup -c",
    "rollup:watch": "rollup -c -w",
    "compile": "tsc --build tsconfig.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "spaced-comment": [
        "error",
        "always",
        {
          "line": {
            "markers": [
              "/",
              "#"
            ]
          },
          "block": {
            "markers": [
              "!",
              "*",
              "#",
              "function",
              "const",
              "if",
              "export",
              "interface",
              "#__PURE__",
              "@__PURE__"
            ],
            "exceptions": [
              "!",
              "*",
              "#"
            ],
            "balanced": false
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-typescript": "^7.7.4",
    "@types/tinymce": "^4.5.24",
    "yargs": "^15.3.1"
  },
  "bic": {
    "skip": [
      "build",
      "build_tsc",
      "README.md",
      "CHANGE_LOG.md"
    ]
  },
  "resolutions": {
    "react-is": "^17.0.2"
  }
}
