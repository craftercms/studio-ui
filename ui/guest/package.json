{
  "name": "@craftercms/experience-builder",
  "description": "CrafterCMS' Experience Builder integration sdk.",
  "version": "4.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/craftercms/studio-ui.git",
    "directory": "ui/guest/src"
  },
  "private": true,
  "main": "build_tsc/node/index.js",
  "module": "build_tsc/index.js",
  "typings": "build_tsc/index.d.ts",
  "craftercms": {
    "id": "craftercms.guest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "spaced-comment": [
        "error",
        "always",
        {
          "line": {
            "markers": [
              "/",
              "#"
            ]
          },
          "block": {
            "markers": [
              "!",
              "*",
              "#",
              "function",
              "const",
              "if",
              "export",
              "interface",
              "#__PURE__",
              "@__PURE__"
            ],
            "exceptions": [
              "!",
              "*",
              "#"
            ],
            "balanced": false
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bic": {
    "skip": [
      "build",
      "build_tsc",
      "README.md",
      "CHANGE_LOG.md"
    ]
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "watch": "rollup -c -w",
    "build:modern": "tsc --build tsconfig.json",
    "clean:npm": "rimraf ./build_tsc ./tsconfig.tsbuildinfo",
    "build:node": "tsc --build tsconfig.node.json",
    "create-package-file": "node ./scripts/package.js",
    "replace-env-version": "node ./scripts/replaceEnvVersion.js",
    "build:npm": "run-s clean:npm build:modern replace-env-version create-package-file copy-jquery-npm",
    "copy-jquery-npm": "cp -r ./src/jquery ./build_tsc/jquery",
    "bom": "mkdir -p target && yarn cyclonedx --output-file target/bom.json"
  },
  "dependencies": {
    "@craftercms/classes": "4.1.4",
    "@craftercms/content": "4.1.4",
    "@craftercms/ice": "4.1.4",
    "@craftercms/npm-content": "workspace:^",
    "@craftercms/search": "4.1.4",
    "@craftercms/studio-ui": "workspace:^",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "@reduxjs/toolkit": "^2.2.5",
    "@rollup/plugin-alias": "^5.1.0",
    "@types/react-dom": "^18.3.0",
    "js-cookie": "^3.0.5",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hotkeys-hook": "^4.5.0",
    "react-is": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-observable": "^3.0.0-rc.2",
    "rxjs": "^7.8.1",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-swc": "^0.3.0",
    "@swc/core": "^1.5.7",
    "@types/jquery": "^3.5.30",
    "@types/js-cookie": "^3.0.6",
    "@types/prop-types": "^15.7.12",
    "@types/react-is": "^18.3.0",
    "build-if-changed": "^1.5.5",
    "npm-run-all": "^4.1.5",
    "replace-in-file": "^7.1.0",
    "rollup": "^4.17.2",
    "rollup-plugin-swc3": "^0.11.2",
    "yargs": "^17.7.2"
  }
}
