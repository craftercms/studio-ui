{
  "name": "@craftercms/experience-builder",
  "description": "Crafter CMS' Experience Builder integration sdk.",
  "version": "1.0.0-beta.3",
  "private": true,
  "main": "build_tsc/node/index.js",
  "module": "build_tsc/index.js",
  "typings": "build_tsc/index.d.ts",
  "craftercms": {
    "id": "craftercms.guest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "spaced-comment": [
        "error",
        "always",
        {
          "line": {
            "markers": [
              "/",
              "#"
            ]
          },
          "block": {
            "markers": [
              "!",
              "*",
              "#",
              "function",
              "const",
              "if",
              "export",
              "interface",
              "#__PURE__",
              "@__PURE__"
            ],
            "exceptions": [
              "!",
              "*",
              "#"
            ],
            "balanced": false
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bic": {
    "skip": [
      "build",
      "build_tsc",
      "README.md",
      "CHANGE_LOG.md"
    ]
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "watch": "rollup -c -w",
    "build:modern": "tsc --build tsconfig.json",
    "clean:npm": "rimraf ./build_tsc ./tsconfig.tsbuildinfo",
    "build:node": "tsc --build tsconfig.node.json",
    "create-package-file": "node ./scripts/package.js",
    "build:package": "run-s clean:npm build:modern build:node create-package-file"
  },
  "dependencies": {
    "@craftercms/classes": "^2.0.4",
    "@craftercms/content": "^2.0.4",
    "@craftercms/ice": "^2.0.4",
    "@craftercms/search": "^2.0.4",
    "@craftercms/studio-ui": "workspace:^",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "@reduxjs/toolkit": "^1.7.0",
    "jquery": "^3.6.0",
    "js-cookie": "^3.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^3.4.4",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.4.0",
    "typescript": "^4.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "7.12.3",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jquery": "^3.5.11",
    "@types/js-cookie": "^3",
    "@types/prop-types": "^15",
    "@types/react-redux": "^7.1.20",
    "@types/tinymce": "^4.6.4",
    "build-if-changed": "^1.5.5",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.59.0",
    "yargs": "^15.3.1"
  }
}
